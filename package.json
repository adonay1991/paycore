{
  "name": "paycore-monorepo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "bun run --filter \"*\" dev",
    "build": "bun run --filter \"*\" build",
    "lint": "ultracite check .",
    "format": "ultracite fix .",
    "test": "bun test",
    "dev:web": "bun run --filter \"@*/web\" dev",
    "dev:platform": "bun run --filter \"@*/platform\" dev",
    "dev:api": "bun run --filter \"@*/api\" dev",
    "debug": "bun --inspect apps/api/src/index.ts",
    "debug:brk": "bun --inspect-brk apps/api/src/index.ts",
    "debug:wait": "bun --inspect-wait apps/api/src/index.ts"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "catalog:"
  },
  "catalog": {
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "next": "^16.0.10",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "hono": "^4.11.1",
    "drizzle-orm": "^0.45.1",
    "drizzle-kit": "^0.31.8",
    "postgres": "^3.4.7",
    "@supabase/supabase-js": "^2.88.0",
    "@supabase/ssr": "^0.8.0",
    "tailwindcss": "^4.1.18",
    "@tailwindcss/postcss": "^4.1.18",
    "postcss": "^8.5.6",
    "autoprefixer": "^10.4.23",
    "radix-ui": "^1.4.3",
    "@radix-ui/react-slot": "^1.2.4",
    "@base-ui/react": "^1.0.0",
    "shadcn": "^3.6.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "tailwind-merge": "^3.4.0",
    "tw-animate-css": "^1.4.0",
    "@phosphor-icons/react": "^2.1.10",
    "iconoir-react": "^7.11.0",
    "ultracite": "^6.4.2",
    "@biomejs/biome": "^2.3.10",
    "vitest": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "@types/node": "^25.0.3",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "framer-motion": "^12.29.0"
  }
}