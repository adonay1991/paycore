{
  "name": "@paycore/api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "bun --hot src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target bun",
    "start": "bun dist/index.js",
    "debug": "bun --inspect src/index.ts",
    "debug:brk": "bun --inspect-brk src/index.ts",
    "debug:wait": "bun --inspect-wait src/index.ts",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:migrate": "drizzle-kit migrate",
    "db:seed": "bun run src/db/seed.ts",
    "db:reset": "bun run src/db/reset.ts",
    "db:setup": "bun run db:push && bun run db:seed"
  },
  "dependencies": {
    "@elevenlabs/elevenlabs-js": "^2.32.0",
    "@hono/zod-validator": "^0.7.6",
    "@paycore/types": "workspace:*",
    "@supabase/supabase-js": "^2.49.1",
    "drizzle-orm": "^0.38.4",
    "hono": "catalog:",
    "postgres": "^3.4.5",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "catalog:",
    "drizzle-kit": "^0.30.4"
  }
}